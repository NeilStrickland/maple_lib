<Group labelreference="L11" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">is_jac := proc(a)
 local x,y,u;
 for x in HH do 
  for y in HH do 
   u := mods([0,0,0,1] +~ mu(x,mu(a,y)),5);
