<Group hide-output="false" labelreference="L31" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">casimir := proc(n::nonnegint,k::nonnegint)
 option remember;
 local i,T,S;
 if k &lt; 0 or k &gt; n then return 0; fi;
 if k = 0 then return bar(); fi;
 if k = 1 then return add(bar(e(i,i)),i=1..n); fi;

 T := [[]];
 for i from 1 to k do 
  T := [seq(seq([op(t),j],j=1..n),t in T)];
 od;

 return add(flow(op(t))/k!,t in T);
