<Group hide-output="false" labelreference="L23" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">bar := proc()
 local n,m,i,j,u,a,b,v,ac,av,x,y,p,q,r,s;
 n := nargs;
 for i from 1 to n do
  u := seq(args[j],j=1..i-1);
  a := args[i];
  v := seq(args[j],j=i+1..n);
  if type(a,`+`) then
   return map(t -&gt; bar(u,t,v),a);
  elif type(a,`*`) then
   ac,av := selectremove(type,a,rational);
   if ac &lt;&gt; 1 then
    return ac * bar(u,av,v);
   fi;
  elif type(a,specfunc(bar)) then
   return bar(u,op(a),v);
  elif type(a,specfunc(e)) then
   m := nops(a);
   if m &lt;&gt; 2 then
    return FAIL;
   fi; 
  fi;
 od;

 for i from 1 to n-1 do
  a := args[i];
  b := args[i+1];
  p := op(1,a); q := op(2,a); r := op(1,b); s := op(2,b);
  if p &gt; r or (p = r and q &gt; s) then
   u := bar(args[1..i-1],b,a,args[i+2..n]);
   if q = r then u := u + bar(args[1..i-1],e(p,s),args[i+2..n]); fi;
   if p = s then u := u - bar(args[1..i-1],e(r,q),args[i+2..n]); fi;
   return u;
  fi;
 od;

 return 'procname'(args);
end:</Text-field>
</Input>
</Group>
